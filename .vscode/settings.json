{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.vscode": false,
        "**/bin": true,
    },
    "cSpell.language": "en,de",
    "cmake.generator": "Unix Makefiles",
    "cmake.configureSettings": {
        //"HARDWARE_PLATFORM":"GOSSENMETRAWATT",
        "HARDWARE_PLATFORM":"VARIKEY",
        "FIRMWARE_PRODUCT":"KEYPAD_DEMO"
    },
    "cSpell.words": [
        "blub",
        "bwdb",
        "BWDB",
        "bytering",
        "Capslock",
        "EPNUM",
        "fifos",
        "GAMEPAD",
        "GMCI",
        "gossenmetrawatt",
        "GPINTENA",
        "gpio",
        "GPIO",
        "GPIOA",
        "GPIOB",
        "GPPUA",
        "HIBYTE",
        "highspeed",
        "HINIBLE",
        "inited",
        "INTA",
        "INTB",
        "INTCAPA",
        "INTCAPB",
        "IODIRA",
        "keymatrix",
        "LDAC",
        "leds",
        "LOBYTE",
        "LONIBLE",
        "LOWORD",
        "Messtechnik",
        "millis",
        "Numlock",
        "OFFT",
        "oled",
        "OLED",
        "OVRFLOW",
        "pico",
        "Pico",
        "picotool",
        "postprocess",
        "pqrs",
        "romsila",
        "RPULL",
        "SCROLLLOCK",
        "SETBYTE",
        "SETHUGE",
        "SETLONG",
        "SETWORD",
        "softkey",
        "termbase",
        "Thach",
        "tinyusb",
        "tusb",
        "uart",
        "usart",
        "uvalue",
        "varikey",
        "Varikey",
        "VARIKEY",
        "vbus",
        "VCOM",
        "vref",
        "VREF",
        "wxyz",
        "XMODEM"
    ],
    "C_Cpp.vcFormat.indent.braces": true,
    "C_Cpp.clang_format_sortIncludes": true,
    "cmake.buildDirectory": "${workspaceFolder}/bin",
    "python.formatting.provider": "yapf",
    "doxdocgen.file.copyrightTag": [
        "Copyright (c) {year}, Roman Koch, koch.roman@gmail.com",
        "SPDX-License-Identifier: MIT"
    ],
    "doxdocgen.file.fileTemplate": "\\file {name}",
    "doxdocgen.generic.authorEmail": "koch.roman@gmail.com",
    "doxdocgen.generic.authorName": "Roman Koch",
    "doxdocgen.generic.authorTag": "\\author {author} ({email})",
    "doxdocgen.generic.briefTemplate": "\\brief {text}",
    "doxdocgen.generic.dateTemplate": "\\date {date}",
    "doxdocgen.generic.paramTemplate": "\\param {param} ",
    "doxdocgen.generic.returnTemplate": "\\return {type} ",
    "doxdocgen.file.fileOrder": [
        "file",
        "author",
        "empty",
        "copyright"
    ],
    "doxdocgen.generic.order": [
        "brief",
        "empty",
        "tparam",
        "param",
        "return",
    ],
    "files.associations": {
        "type_traits": "cpp",
        "*.tcc": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "deque": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "cstdint": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "unordered_map": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "iosfwd": "cpp",
        "ostream": "cpp",
        "ranges": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "cstring": "cpp",
        "istream": "cpp"
    },
    "workbench.preferredDarkColorTheme": "Default Dark+",
    "workbench.colorTheme": "Default Dark+",
    "markdown.extension.print.theme": "light",
    "markdown-mermaid.darkModeTheme": "default"
}