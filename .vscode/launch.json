{
    "version": "0.2.0",
    "showDevDebugOutput": "raw",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "name": "J-Link",
            "executable": "${command:cmake.launchTargetPath}",
            "serverpath": "/opt/SEGGER/JLink/JLinkGDBServer",
            "servertype": "jlink",
            "device": "RP2040_M0_0",
            "interface": "swd",
            "serialNumber": "0",
            "gdbPath": "gdb-multiarch",
            "armToolchainPath": "/usr/bin/",
            "svdFile": "${workspaceFolder}/bin/_deps/pico_sdk-src/src/rp2040/hardware_regs/rp2040.svd",
            "postRestartCommands": [
                "break main",
                "continue"
            ],
            "showDevDebugOutput": "none"
        },
        {
            "name": "Picoprobe",
            "gdbPath": "gdb-multiarch",
            "cwd": "${workspaceRoot}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "RP2040",
            "configFiles": [
                "interface/picoprobe.cfg",
                "target/rp2040.cfg"
            ],
            "svdFile": "${env:PICO_SDK_PATH}/src/rp2040/hardware_regs/rp2040.svd",
            "postRestartCommands": [
                "break main",
                "continue"
            ]
        },
        {
            "name": "Picoprobe2",
            "gdbPath": "gdb-multiarch",
            "cwd": "${workspaceRoot}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "RP2040",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2040.cfg"
            ],
            "svdFile": "${env:PICO_SDK_PATH}/src/rp2040/hardware_regs/rp2040.svd",
            "postRestartCommands": [
                "break main",
                "continue"
            ]
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "name": "rPi-SWD",
            "executable": "${command:cmake.launchTargetPath}",
            "gdbPath": "gdb-multiarch",
            "servertype": "openocd",
            "device": "RP2040",
            "configFiles": [
                "interface/raspberrypi-swd.cfg",
                "target/rp2040.cfg"
            ],
            "svdFile": "${workspaceFolder}/bin/_deps/pico_sdk-src/src/rp2040/hardware_regs/rp2040.svd",
            "postRestartCommands": [
                "break main",
                "continue"
            ]
        },
        {
            "name": "fonts",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/tools/converter",
            "program": "fonts.py",
            "justMyCode": true
        },
        {
            "name": "images",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/tools",
            "program": "images.py",
            "justMyCode": true
        },
        {
            "name": "commnader",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/tools/commander",
            "module": "commander",
            "justMyCode": true
        }
    ]
}